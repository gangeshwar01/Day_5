{"cells": [{"cell_type": "code", "execution_count": null, "metadata": {}, "outputs": [], "source": ['# modeling.ipynb', 'import pandas as pd', 'import statsmodels.api as sm', '', '# Load cleaned data', 'df = pd.read_csv("data/cleaned_data.csv")', '', '# One-hot encode categorical variables', 'df = pd.get_dummies(df, columns=["region", "gender"], drop_first=True)', '', '# Define features and target', 'X = df[["age", "income_per_age", "log_expenses"] + [col for col in df.columns if "region_" in col or "gender_" in col]]', 'X = sm.add_constant(X)', 'y = df["income"]', '', '# Fit linear regression model', 'model = sm.OLS(y, X).fit()', '', '# Print summary', 'print(model.summary())']}], "metadata": {}, "nbformat": 4, "nbformat_minor": 2}